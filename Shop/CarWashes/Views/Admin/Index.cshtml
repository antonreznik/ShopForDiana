
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Admin/_AdminLayoutPage.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col s2 valign-wrapper center-align blue lighten-2 hoverable div_style" id="add_category">
            <span>Добавить категорию товаров</span>
        </div>

        <div class="col s2 valign-wrapper center-align blue lighten-2 hoverable div_style">
            @Html.ActionLink("Отредактировать наименование категории товаров", "EditCategory", "Category")
        </div>

        <div class="col s2 valign-wrapper center-align blue lighten-2 hoverable div_style">
            @Html.ActionLink("Скрыть категорию товаров", "HideCategory", "Category")
        </div>

        <div class="col s2 valign-wrapper center-align blue lighten-2 hoverable div_style">
            @Html.ActionLink("Показать категорию товаров", "AddWasher", "Admin")
        </div>

        <div class="col s2 valign-wrapper center-align blue lighten-2 hoverable div_style">
            @Html.ActionLink("Добавить категорию товаров", "AddWasher", "Admin")
        </div>

        <div class="col s2 valign-wrapper center-align blue lighten-2 hoverable div_style">
            @Html.ActionLink("Добавить категорию товаров", "AddWasher", "Admin")
        </div>
    </div>

    <div id="modal_window" class="modal modal-close">
        <div class="modal-header right">
            <button class="modal-close btn red accent-2">
                <i class="material-icons">close</i>
            </button>
        </div>
        <div class="modal-content" id="modal_content"></div>
    </div>
</div>





